<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android"
    >


    <item  >
        <shape
            >
            <stroke android:width="1.2dp" android:color="#FFFFFF" />
            <padding android:bottom="1.2dp"
                android:left="1.2dp"
                android:right="1.2dp"
                android:top="1.2dp"
                />
            <corners android:radius="3dp"/>
            <size android:width="90dp" android:height="90dp"/>
        </shape>
    </item>


    <item >
        <shape
            >
            <stroke android:width="1.5dp" android:color="#457229"/>
            <padding android:bottom="1.5dp"
                android:left="1.5dp"
                android:right="1.5dp"
                android:top="1.5dp"
                />
            <corners android:radius="3dp"/>
            <size android:width="90dp" android:height="90dp"/>
        </shape>
    </item>



    <item >
        <shape >

            <gradient
                android:angle="135"
                android:endColor="#73CC3D"
                android:startColor="#64AF35"
                />
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp"
                />
            <corners android:radius="3dp"/>
            <size android:width="90dp" android:height="90dp"/>
        </shape>
    </item>

    <item >
        <shape  >

            <solid android:color="#11FFFFFF"
                />
            <corners android:radius="3dp"/>
        </shape>
    </item>

</layer-list>